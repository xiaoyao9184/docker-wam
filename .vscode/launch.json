{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debugpy: watermark-anything gradio",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/gradio/gradio_run.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "${workspaceFolder}/gradio/gradio_app.py"
            ],
            "env": {
                "APP_PATH": "${workspaceFolder}/watermark-anything",
                "PYDEVD_DISABLE_FILE_VALIDATION": "1",
                "XDG_CACHE_HOME": "${workspaceFolder}/cache",
            },
            "justMyCode": false
        },
        {
            "name": "debugpy: watermark-anything streamlit",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/watermark-anything",
            "args": [
                "run",
                "${workspaceFolder}/streamlit/streamlit_app.py"
            ],
            "env": {
                "STREAMLIT_SERVER_PORT": "8501"
            },
            "justMyCode": false
        }
    ]
}